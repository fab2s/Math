{
    "name" : "fab2s/math",
    "description" : "A Base10 high precision math helper",
    "type" : "library",
    "authors" : [{
        "name" : "Fabrice de Stefanis"
    }],
    "keywords" : [
        "PHP",
        "Simple",
        "Base10",
        "decimal",
        "BcMath",
        "HighPrecision"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": "^8.1",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.11",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "classmap": [
          "src"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "fab2s\\Math\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "rm -rf .*.cache"
        ],
        "post-install-cmd": [
            "rm -rf .*.cache"
        ],
        "fix": "@php vendor/bin/pint --config pint.json"
    },
    "suggest": {
        "ext-gmp": "For faster Math::baseConvert up to base62"
    }
}
